    %link{href: 'stylesheets/bootstrap.min.css', rel: 'stylesheet', type: 'text/css'}
    %link{href: 'stylesheets/jquery.Jcrop.min.css', rel: 'stylesheet', type: 'text/css'}
    %link{href: 'stylesheets/main.css', rel: 'stylesheet', type: 'text/css'}
  %body
    #nav.navbar.navbar-default
      %div.navbar-header
        %a.navbar-brand{ href: "/" }
          = SITECONFIG.title

    #content
      = yield

    #footer

    %script{ src: "http://code.jquery.com/jquery-1.10.2.min.js", type: "text/javascript" }
    %script{ src: "javascripts/jquery.Jcrop.min.js", type: "text/javascript" }
    %script{ src: "javascripts/jquery.form.min.js", type: "text/javascript" }
    %script{ src: "javascripts/script.js", type: "text/javascript" }


